<div id="modal-addCustomer" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content radius">
      <div class="modal-header">
        <h5 class="modal-title">新增客户</h5>
        <a class="close" data-dismiss="modal" aria-hidden="true" href="javascript:void();">×</a>
      </div>
      <form class="form form-horizontal" id="addCustomerForm">
        <div class="modal-body">
          <div class="row cl">
            <label class="form-label col-xs-4 col-sm-3">客户名称：</label>
            <div class="formControls col-xs-8 col-sm-8">
              <input type="text" class="input-text" placeholder="客户名称" name="companyName" id="companyName">
            </div>
          </div>
          <div class="row cl">
            <label class="form-label col-xs-4 col-sm-3">客户类型：</label>
            <div class="formControls col-xs-8 col-sm-8">
              <select name="typeId" id="typeId">
                    <option value="">请选择客户类型</option>
                  </select>
              <input type="hidden" name="typeName" id="typeName" value="">
            </div>
          </div>
          <div class="row cl">
            <label class="form-label col-xs-4 col-sm-3">联系人：</label>
            <div class="formControls col-xs-8 col-sm-8">
              <input type="text" class="input-text" autocomplete="off" placeholder="联系人" name="linkMan" id="linkMan">
            </div>
          </div>
          <div class="row cl">
            <label class="form-label col-xs-4 col-sm-3">手机号码：</label>
            <div class="formControls col-xs-8 col-sm-8">
              <input type="text" class="input-text" autocomplete="off" placeholder="手机号码" name="phone" id="phone">
            </div>
          </div>
          <div class="row cl">
            <label class="form-label col-xs-4 col-sm-3">接口：</label>
            <div class="formControls col-xs-8 col-sm-8">
              <label class="radio-inline">
                <input type="radio" name="isNeedCall" id="inlineRadio1" value="0" checked> 不使用
              </label>
              <label class="radio-inline">
                <input type="radio" name="isNeedCall" id="inlineRadio2" value="1"> 使用
              </label>
            </div>
          </div>
          <div class="row cl active" id="rebackurl">
            <label class="form-label col-xs-4 col-sm-3">回调地址：</label>
            <div class="formControls col-xs-8 col-sm-8">
              <input type="text" class="input-text" autocomplete="off" placeholder="回调地址" name="callBackUrl" id="backlink">
            </div>
          </div>
          <div class="row cl">
            <label class="form-label col-xs-4 col-sm-3">省市：</label>
            <div class="formControls col-xs-8 col-sm-9">
              <div id="province_city"></div>
              <!-- <input type="hidden" id="customer_province" name="province" value="">
                  <input type="hidden" id="customer_city" name="city" value=""> -->
            </div>
          </div>
          <div class="row cl">
            <label class="form-label col-xs-4 col-sm-3">详细地址：</label>
            <div class="formControls col-xs-8 col-sm-8">
              <textarea class="textarea" placeholder="详细地址..." rows="" cols="" name="address" id="address"></textarea>
            </div>
          </div>
          <div class="row cl">
            <label class="form-label col-xs-4 col-sm-3">LOGO：</label>
            <div class="formControls col-xs-8 col-sm-8" id="uploadFileWrapper">
              {{!-- <i class="Hui-iconfont uploadIcon">&#xe604</i> --}}
              <a id="addLogoText">添加LOGO</a>
              <input type="file" class="input-text" autocomplete="off" placeholder="请上传LOGO" id="fileUpload" accept="image/png,image/jpg,image/jpeg">
              <input type="text" name="logoUrl" id="logoUrl">
              <img id="fileImg" src="" alt="">
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <input class="btn btn-primary" type="submit" value="提交">
          <!-- <button type="submit" class="btn btn-primary">确定</button> -->
          <button class="btn" data-dismiss="modal" aria-hidden="true">关闭</button>
        </div>
      </form>
    </div>
  </div>
</div>