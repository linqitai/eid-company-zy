
<table class="table table-border table-bordered table-hover">
    <thead class="text-c">
        <tr>
            <th>店铺</th>
            <th>查询时间</th>
            <th>身份核验次数</th>
            <th>EID报告次数</th>
            <th>总金额</th>
        </tr>
           {{#if this}}

            <tr class="text-c">
                <td>总计</td>
                <td>{{ this.obj.queryTotal.start}}至{{ this.obj.queryTotal.end}}</td>
                <td>{{this.obj.queryTotal.identityTime}}</td>
                <td>{{this.obj.queryTotal.reportTime}}</td>
                <td>
                   {{this.obj.queryTotal.amount}} 
                </td>
            </tr>
    {{else}}
        <tr>
            <td class="text-c" colspan="5"></td>
        </tr>
    {{/if}}

    </thead>
    <tbody class="text-c">
    {{#if this}}
        {{#each this.list}}
            <tr class="text-c">
                <td>{{companyName}}</td>
                <td> <a class="detailBtn underLine" href="/headShop/count/detail.htm?companyId={{companyId}}&start={{ start}}&end={{ end}}&queryDateType={{queryDateType}}">{{ start}}至{{ end}}</a></td>
                <td class="sureinfo"><a class="detailBtn underLine" href="/headShop/infoSearch/view.htm?companyId={{companyId}}&_startTime={{ start}}&_endTime={{ end}}&queryDateType={{queryDateType}}">{{identityTime}}</a></td>
                <td><a class="detailBtn underLine" href="/headShop/infoSearch/view.htm?companyId={{companyId}}&_startTime={{ start}}&_endTime={{ end}}&queryDateType={{queryDateType}}&businessProgress=10">{{reportTime}}</a></td>
                <td>
                   {{amount}} 
                </td>
            </tr>
        {{/each}}
    {{else}}
        <tr>
            <td class="text-c" colspan="8">查无数据</td>
        </tr>
    {{/if}}
    </tbody>
</table>