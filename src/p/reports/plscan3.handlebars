<table class="record mt40">
    <thead class="text-c">
        <tr>
            <td colspan="5">信贷不良信息</td>
        </tr>
    </thead>
    <tbody>
        {{#if this}} {{#each this}}
        <tr class="text-c">
            {{#each this}}
            <td>{{this}}</td>
            {{/each}}
        </tr>
        {{/each}} {{else}}
        <tr>
            <td class="text-c" colspan="8">查无数据</td>
        </tr>
        {{/if}}
    </tbody>
</table>
