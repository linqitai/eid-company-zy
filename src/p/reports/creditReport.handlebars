<div class="cl top_border">
    <div class="bottom_btn">
      <a class="btn btn-primary radius download" href="javascript:void(0)">下载报告</a>
      <button class="btn btn-primary {{#ifCond status "!=" 0}}disabled{{/ifCond}} radius pass" {{#ifCond status "!=" 0}}disabled{{/ifCond}}>审核通过</button>
      <button class="btn btn-primary {{#ifCond status "!=" 0}}disabled{{/ifCond}} radius noThrougth" {{#ifCond status "!=" 0}}disabled{{/ifCond}}>审核不通过</button>
      <button class="btn btn-primary {{#ifCond status "!=" 0}}disabled{{/ifCond}} radius blacklist" {{#ifCond status "!=" 0}}disabled{{/ifCond}}>加入黑名单</button>
    </div>
</div>