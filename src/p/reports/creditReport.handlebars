<div class="cl number_time">
<!-- <div class="col-sm-6">报告编号：201611190000001</div> -->
<div class="col-sm-12 text-r">查询时间：{{formatDate created}}</div>
</div>
<h4 class="text-c">查询信息</h4>
<table class="table table-border table-bordered table-hover table-bg">
    <thead class="text-c">
      <tr>
        <th>被查询者姓名</th>
        <th>被查询者证件类型</th>
        <th>被查询者证件号码</th>
        <th>查询门店</th>
      </tr>
    </thead>
    <tbody class="text-c">
      <tr>
        <td>{{borrowerName}}</td>
        <td>身份证</td>
        <td>{{cardNum}}</td>
        <td>{{companyName}}</td>
      </tr>
    </tbody>
</table>
<h4 class="text-c">人证核实</h4>
<table class="table table-border table-bordered table-hover table-bg">
    <thead class="text-c">
      <tr>
        <th>身份证正面</th>
        <th>身份证反面</th>
        <th>本人照片</th>
      </tr>
    </thead>
    <tbody class="text-c">
        <tr>
            <td>
                <div class="identity identityFront">
                    <div class="cl mt20">
                        <div class="pull-left w170 ml-5">
                            <div class="lineBox">
                                <span class="lightblue">姓名</span><span class="addText">{{#if borrowerName}}{{borrowerName}}{{else}}--{{/if}}</span>
                            </div>
                            <div class="lineBox">
                                <span class="lightblue">性别</span><span class="addText">{{#if sex}}{{infoSex sex}}{{else}}--{{/if}}</span><span class="ml-15 lightblue">民族</span><span class="addText">{{#if nation}}{{nation}}{{else}}--{{/if}}</span>
                            </div>
                            <div class="lineBox">
                                <span class="lightblue">出生</span><span class="addText birthdate_year"></span><span class="lightblue"> 年 </span><span class="addText birthdate_mouth"></span><span class="lightblue"> 月 </span><span class="addText birthdate_day"></span><span class="lightblue"> 日 </span>
                            </div>
                            <div class="lineBox">
                                <span class="lightblue">住址</span><span class="addText">{{#if address}}{{address}}{{else}}--{{/if}}</span>
                            </div>
                        </div>
                        <div class="pull-right w80 mr-5 mt5">
                           <img src="{{imgUrl}}" class="frontPic" alt="暂无此照片">
                        </div>
                    </div>
                    <div class="lineBox pdt18 cl ml-5">
                        <span class="lightblue">公民身份证号码</span><span class="addText">{{#if cardNum}}{{cardNum}}{{else}}--{{/if}}</span>
                    </div>
                </div>
            </td>
            <td>
                <div class="identity identityBack">
                    <div class="lineBox mt125 ml-50">
                        <span class="fw_bold">签发机关</span><span class="addText">{{#if issuer}}{{issuer}}{{else}}--{{/if}}</span>
                    </div>
                    <div class="lineBox ml-50">
                        <span class="fw_bold">有效期限</span><span class="addText">{{#if validDate}}{{validDate}}{{else}}--{{/if}}</span>
                    </div>
                </div>
            </td>
            <td>
                <img class="card_img" src="{{faceUrl}}">
            </td>
        </tr>
        <tr>
          <td class="td_bg">核实结果</td>
          <td colspan=2>{{#ifCond compareResult "==" 0}}人脸比对成功{{else}}人脸比对失败{{/ifCond}}</td>
        </tr>
    </tbody>
</table>
<h4 class="text-c">法院数据</h4>
<table class="table table-border table-bordered table-hover table-bg">
    <thead class="text-c">
      <tr>
        <th>法院失信名单</th>
        <th>法院执行名单</th>
        <th>法院结案</th>
        <th>法人失信</th>
      </tr>
    </thead>
    <tbody class="text-c courtDataFirst">

    </tbody>
</table>
<table class="table table-border table-bordered table-hover table-bg">
    <thead class="text-c">
      <tr>
        <th>案号</th>
        <th>执行法院</th>
        <th>案件状态</th>
      </tr>
    </thead>
    {{#if this.courtList}}
        <tbody class="text-c courtDataSecond">

        </tbody>
    {{else}}
        <tbody class="text-c">
            <tr>
              <td>--</td>
              <td>--</td>
              <td>--</td>
            </tr>
        </tbody>
    {{/if}}
</table>
<h4 class="text-c">信贷行业</h4>
<table class="table table-border table-bordered table-hover table-bg">
    <thead class="text-c">
      <tr>
        <th>信用/消费贷黑名单</th>
        <th>信贷逾期</th>
        <th>信用卡逾期</th>
        <th>P2P黑名单</th>
        <th>助学贷款逾期</th>
      </tr>
    </thead>
    <tbody class="text-c creditFirst">

    </tbody>
</table>
<table class="table table-border table-bordered table-hover table-bg">
    <thead class="text-c">
      <tr>
        <th>套现交易</th>
        <th>信贷失联</th>
        <th>信贷欺诈</th>
        <th>风控规则</th>
        <th>黑中介</th>
      </tr>
    </thead>
    <tbody class="text-c creditSecond">

    </tbody>
</table>
<table class="table table-border table-bordered table-hover table-bg">
    <thead class="text-c">
      <tr>
        <th>骗取补贴</th>
        <th>冒用风险</th>
        <th>信贷分值低</th>
        <th>曾经逾期（0-30天）</th>
        <th>信贷黑名单</th>
      </tr>
    </thead>
    <tbody class="text-c creditThree">

    </tbody>
</table>
<table class="table table-border table-bordered table-hover table-bg">
    <thead class="text-c">
      <tr>
        <th>商户欺诈名单</th>
        <th>曾经逾期（未知期限）</th>
      </tr>
    </thead>
    <tbody class="text-c creditFour">

    </tbody>
</table>
<h4 class="text-c">三方支付</h4>
<table class="table table-border table-bordered table-hover table-bg">
    <thead class="text-c">
      <tr>
        <th>盗卡</th>
        <th>其他欺诈</th>
        <th>盗用操作</th>
        <th>盗用支出</th>
      </tr>
    </thead>
    <tbody class="text-c threePayment">

    </tbody>
</table>
<h4 class="text-c">典当行</h4>
<table class="table table-border table-bordered table-hover table-bg">
    <thead class="text-c">
      <tr>
        <th>典当物品</th>
        <th>典当金额</th>
        <th>典当地点</th>
        <th>典当时间</th>
      </tr>
    </thead>
    {{#if this.pawnList}}
        <tbody class="text-c pawnshopFirst">

        </tbody>
    {{else}}
        <tbody class="text-c">
            <tr>
              <td>--</td>
              <td>--</td>
              <td>--</td>
              <td>--</td>
            </tr>
        </tbody>
    {{/if}}
</table>
<table class="table table-border table-bordered table-hover table-bg">
    <thead class="text-c">
      <tr>
        <th>违约行为</th>
        <th>描述</th>
      </tr>
    </thead>
    {{#if this.pawnList}}
        <tbody class="text-c pawnshopSecond">

        </tbody>
    {{else}}
        <tbody class="text-c">
            <tr>
              <td>--</td>
              <td>--</td>
            </tr>
        </tbody>
    {{/if}}
</table>
<h4 class="text-c">负面信息</h4>
<table class="table table-border table-bordered table-hover table-bg">
    <thead class="text-c">
      <tr>
        <th>不良记录</th>
        <th>案件时间</th>
      </tr>
    </thead>
    <tbody class="text-c">
      <tr>
        <td>{{#if message}}{{message}}{{else}}--{{/if}}</td>
        <td>{{#if caseTime}}{{caseTime}}{{else}}--{{/if}}</td>
      </tr>
    </tbody>
</table>
